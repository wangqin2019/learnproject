* {
	box-sizing: border-box;
	margin: 0;
	padding: 0;
	list-style: none;
	outline: none;
}
body{
	background: #303132;
	min-width: 1200px;
}

.header {
	width: 100%;
	overflow: hidden;
	padding: 10px 40px;
	background: #d80e39;
	position: relative;
}

.header img {
	width: 160px;
	height: 62px;
	display: block;
	float: left;
}

.header button {
	width: 96px;
	height: 30px;
	border: none;
	border-radius: 2em;
	background: #fff;
	color: #d80e39;
	text-align: center;
	line-height: 30px;
	position: absolute;
	right: 40px;
	top: 50%;
	margin-top: -15px;
}
.play_zb_msg{
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;	
	z-index: 10;
}
.touxiang {
	width: 100%;
	padding: 1em 0 1em 1em;
	overflow: hidden;
}

.touxiang_img {
	width: 3em;
	height: 3em;
	float: left;
	border-radius: 50%;
	margin-right: 1em;
}
.loction {
	overflow: hidden;
	width: 11em;
}

.loction i,.loction div {
	color: #FFFFFF;
	overflow: hidden;
	line-height: 1.5em;
}

.loction i {
	font-size: 18px;
	font-style:normal
}
.loction div{
	display: flex;
}
.loction div i {
	font-size: 14px;
	float: left;
	font-style:normal
}

.loction div img {
	width: 0.6em;
	height: 0.8em;
	margin-top: 0.2em;
	margin-right: 0.5em;
}

.zhibo_p{
	width: 100%;
	text-align: center;
	font-size: 1.5em;
	color: #fff;
	font-style:normal;
	z-index: 10;
	margin-bottom: 2em;
	margin-top: 1em;
}

.zhibo_p2{
	width: 100%;
	text-align: center;
	font-size: 1em;
	color: #fff;
	font-style:normal;
}

.renshu{
	width: 50%;
	position: absolute;
	left: 5%;
	bottom: 5%;
	overflow: hidden;
	z-index: 10;
}
.renshu div{
	display: flex;
}
.renshu i,.renshu img{
	color: #fff;
	font-style:normal;
}

.main_banner .renshu img{
	width: 22px;
	height: 22px;
	display: block;
    margin: 1px 5px 0 0;
}



.btn_zhibo{
	padding: 0.3em 1.2em 0.5em 0.5em;
	background: #da0f3b;
	border-radius: 2em;
	position: absolute;
	right: 1em;
	bottom: 1.5em;
	z-index: 10;
}
@media only screen and (max-width:1400px ) {
	.btn_zhibo{padding: 0.2em 1em 0.3em 0.2em;}
}


.yuandian{
	width: 8px;
	height: 8px;
	border-radius: 50%;
	background: #fff;
	float: left;
	margin: 7px 8px 0 13px;
}

.zhiboz{
	font-style:normal;
	color: #fff;
}

.main_banner_box{
	width: 100%;
	overflow: hidden;
	margin-top: 2em;
}

.main_banner_box li{
	float: left;
	position: relative;
	width: 16em;
	margin-bottom: 2em;
	margin-left: 3%;
}

.main_banner_box li .li_img{
	width: 100%;
	height: 26em;
	display: block;
	margin: 0 auto;
	background-size: cover !important;
	background-position: center !important;
	background-repeat: no-repeat !important;
}

.mengban{
	width: 100%;
	height: 100%;
	position: absolute;
	left: 0;
	top: 0;
	background: rgba(0,0,0,0.3);
	z-index: 5;
}

/*20200414*/
.zhibo_btn{
	position: absolute;
	top: 5px;
	right: 5px;
	line-height: 20px;
	color: #fff;
	text-align: center;
	width: 80px;
	border-radius: 10px;
	background: #C7B271;
	font-size: 12px;
	cursor: pointer;
	z-index: 999;
}
.wrapper{
	position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 998;
    background: #000;
}
.wrapper video,.wrapper .video{width: 100%;max-height: 100% !important;}

.wrapper .video div,.wrapper .video button,.hide{display: none;}
.voice_btn{
	position: absolute;
	top: 5px;
	left: 5px;
	width: 20px;
	cursor: pointer;
	z-index: 999;
}
