	<script type="text/javascript">
		var navid = getQueryString('navid');
		navChange(navid);
		$('.nav p').click(function(){
			let num = $(this).attr('data-num')
			navChange(num);
		})
		
		function navChange(num){
			$('.part').addClass('hide');
			$('.nav p').removeClass('click');
			if(num == 1){$('.part_one').removeClass('hide');$('.navone').addClass('click');}
			else if(num == 2){$('.part_two').removeClass('hide');$('.navtwo').addClass('click');}
			else if(num == 3){$('.part_three').removeClass('hide');$('.navthree').addClass('click');}
		}
		//获取地址
		function getQueryString(name) {
		  var reg = new RegExp('(^|&)' + name + '=([^&]*)(&|$)', 'i');
		  var r = window.location.search.substr(1).match(reg);
		  if (r != null) {
		    return unescape(r[2]);
		  }
		  return null;
		}
	</script>
