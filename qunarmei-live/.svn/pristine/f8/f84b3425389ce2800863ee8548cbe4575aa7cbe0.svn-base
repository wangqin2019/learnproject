 html, body {
        position: relative;
        height: 100%;
        -webkit-text-size-adjust: 100%;-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    }
    body {
        margin: 0;
        padding: 0;
    }
    .swiper-container {
        width: 100%;
        height: 100%;
    }
    .swiper-slide {

        /* Center slide text vertically */
        display: -webkit-box;
        display: -ms-flexbox;
        display: -webkit-flex;
        display: flex;
        -webkit-box-pack: center;
        -ms-flex-pack: center;
        -webkit-justify-content: center;
        justify-content: center;
        -webkit-box-align: center;
        -ms-flex-align: center;
        -webkit-align-items: center;
        align-items: center;
        position: relative;
    }
    .swiper-slide img{
    	height: 100%;
    	display: block;
    }
    
    .swiper-slide .bjimg{
    	width: 100%;
    	display: block;
    	height: auto;
    }

	.swiper-slide .bjimg2{
		-o-animation:scales 1s linear forwards;
		-moz-animation:scales 1s linear forwards;
		-webkit-animation: scales 1s linear forwards;
		animation: scales 1s linear forwards;
	}
	@-ms-keyframes scales {
		from{
			-webkit-transform: scale(0);
			-moz-transform: scale(0deg);
			-ms-transform: scale(0deg);
			-o-transform: scale(0deg);
	    	transform: scale(0deg);
		}
		to{
			-webkit-transform: scale(1);
			-moz-transform: scale(1);
			-ms-transform: scale(1);
			-o-transform: scale(1);
			transform: scale(1);
		}
	}
	
	@-moz-keyframes scales {
		from{
			-webkit-transform: scale(0);
			-moz-transform: scale(0deg);
			-ms-transform: scale(0deg);
			-o-transform: scale(0deg);
	    	transform: scale(0deg);
		}
		to{
			-webkit-transform: scale(1);
			-moz-transform: scale(1);
			-ms-transform: scale(1);
			-o-transform: scale(1);
			transform: scale(1);
		}
	}
	
	@-webkit-keyframes scales {
		from{
			-webkit-transform: scale(0);
			-moz-transform: scale(0deg);
			-ms-transform: scale(0deg);
			-o-transform: scale(0deg);
	    	transform: scale(0deg);
		}
		to{
			-webkit-transform: scale(1);
			-moz-transform: scale(1);
			-ms-transform: scale(1);
			-o-transform: scale(1);
			transform: scale(1);
		}
	}
	@keyframes scales {
	    from{
			-webkit-transform: scale(0);
			-moz-transform: scale(0deg);
			-ms-transform: scale(0deg);
			-o-transform: scale(0deg);
	    	transform: scale(0deg);
		}
		to{
			-webkit-transform: scale(1);
			-moz-transform: scale(1);
			-ms-transform: scale(1);
			-o-transform: scale(1);
			transform: scale(1);
		}
	}

