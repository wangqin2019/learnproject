*{
	/*touch-action: none;*/
}
.hide{display: none;}
.navtopbar{
	background: #F0E1C2;
}
.navtopbar p{
	font-size: 1.2em;
}
.navtopbar{
	position: fixed;
	top: 0;
	left: 0;
}
/*.window{
	margin-top: 3.4em;
